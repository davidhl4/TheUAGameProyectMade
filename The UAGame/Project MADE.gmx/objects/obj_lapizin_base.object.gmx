        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(vida &gt; 0){
    if(place_meeting(x, y+1, obj_solid)) {
        vspd = 0;
    }else if(place_meeting(x, y+1, obj_semiblock) &amp;&amp; !place_meeting(x, y, obj_semiblock)){
        if(vspd &gt;= 0){
            vspd = 0;   
        }
    }else{
        //Gravedad
        if(vspd &gt; 0){
            vspd += 1.25*grav;
        }    
        if(vspd &lt;= 0){
            vspd += grav;
        }  
    }
    
    if(place_meeting(x+hspd, y, obj_solid)){
        while(!place_meeting(x+sign(hspd), y, obj_solid)){
            x += sign(hspd);
        }
        hspd = 0;
    }else{
    
        if(obj_player.x+450 &lt; x){ // Camina hacia la izquierda
            hspd = -3;
            direccion=-1;
        }else if(obj_player.x-450 &gt; x){ //Camina hacia la derecha
            hspd = 3;
            direccion=1;
        }else{
            hspd = 0;
        }
    
    }
    // Colisiones verticales
    if(place_meeting(x, y+vspd, obj_solid)){
        while(!place_meeting(x, y+sign(vspd), obj_solid)){
            y += sign(vspd);
        }
        vspd = 0;
    }else if(place_meeting(x, y+(vspd), obj_semiblock) &amp;&amp; !place_meeting(x, y, obj_semiblock) &amp;&amp; (vspd &gt;= 0)){
        while(!place_meeting(x, y+sign(vspd), obj_semiblock)){
            y += sign(vspd);
        }
        vspd = 0;
    }
    
    
    
    y += vspd;
    x += hspd;
    
    
if((obj_player.x+600 &gt; x) &amp;&amp; canshoot){
    alarm[1] = 1;
    instance_create(x, y, obj_balalapiz);
    canshoot = false;
    alarm[0] = 200 + random(60);
}else if(obj_player.x-600 &lt; x &amp;&amp; canshoot){
    alarm[1] = 1;
    instance_create(x, y, obj_balalapiz);
    canshoot = false;
    alarm[0] = 200 + random(60);
}
    
    
    
    
    
    
    
    
    
    
    
    
    
    //-------------------------------------------------------
    // =====================  SPRITES  =====================
    //-------------------------------------------------------
    
    
    
    
    if(hspd&lt;0){
        sprite_index = spr_lapizin_b;
        image_xscale = 1;
    }else if(hspd&gt;0){
        sprite_index = spr_lapizin_b;
        image_xscale = -1;
    }else{
        if(x &lt;= obj_player.x){
            sprite_index = spr_lapizin;
            image_xscale = -1;
        }else{
            sprite_index = spr_lapizin;
            image_xscale = 1;
        }
    }
}
</string>
